{
  "name": "backend-cashstream",
  "version": "1.0.0",
  "description": "Backend API for CashStream",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cd admin && yarn build",
    "build:backend": "tsc",
    "build:admin": "cd admin && yarn build",
    "start": "node dist/index.js",
    "predev": "node scripts/clean-dev.js",
    "dev": "nodemon",
    "dev:admin": "cd admin && yarn dev",
    "dev:all": "concurrently \"yarn dev\" \"yarn dev:admin\"",
    "type-check": "tsc --noEmit",
    "dev:clean": "pkill -f 'node.*3000' || true; sleep 2; yarn dev",
    "install:all": "yarn install && cd admin && yarn install",
    "migrate": "ts-node scripts/runMigration.ts",
    "migrate:all": "ts-node --esm scripts/runAllMigrations.ts"
  },
  "keywords": [
    "express",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.15.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^7.0.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.1",
    "@types/node-telegram-bot-api": "^0.64.12",
    "@types/nodemailer": "^7.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^11.0.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
